<h1>@ViewBag.title</h1>

<form method="post">
    @foreach (var agent in ViewBag.agents)
    {
        <input type="checkbox" name="AgentIds" id="@agent.AgentId" value="@agent.AgentId" />
        <label for="@agent.AgentId">@agent.Name</label>
        <br />

    }
    <input type="submit" value="Remove Selected Agent(s)" />
</form>